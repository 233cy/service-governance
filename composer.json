{
    "name": "hyperf/service-governance",
    "description": "A service governance component for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "service-governance"
    ],
    "support": {
    },
    "require": {
        "php": ">=7.2",
        "hyperf/consul": "dev-master",
        "hyperf/contract": "dev-master",
        "hyperf/event": "dev-master",
        "hyperf/guzzle": "dev-master"
    },
    "require-dev": {
        "malukenho/docheader": "^0.1.6",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0.0",
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "suggest": {
    },
    "autoload": {
        "files": [
        ],
        "psr-4": {
            "Hyperf\\ServiceGovernance\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
        },
        "hyperf": {
            "config": "Hyperf\\ServiceGovernance\\ConfigProvider"
        }
    },
    "bin": [
    ],
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "test": "phpunit --colors=always"
    }
}
